/* zoom effect hero images */
.hero__img {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    object-fit: cover;
    opacity: 0;
    transition: all 1s;
    transform: scale(1);
    filter: blur(0px);
}

.hero__img.is-showing {
    opacity: 1;
    animation: hero-zoom 3s ease-in-out 0.5s forwards;
}

@keyframes hero-zoom {
    0% {
        /* filter: blur(0px); */
        transform: scale(1);
    }
    100% {
        /* filter: blur(4px); */
        transform: scale(1.2);
    }
}

