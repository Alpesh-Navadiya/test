    }
    .navbar-mobile a#dropdowncontact{
        padding-left: 0;
        padding: 0;
    }

    #hamburger {
        cursor: pointer;
        position: absolute;
        z-index: 6;
        top: 11px;
        right: 12px;
        width: 32px;
        height: 32px;
    }

    #hamburger__line-1,
    #hamburger__line-2,
    #hamburger__line-3 {
        border-top: 2px solid white;
        margin-top: 6px;
        transition: all 0.2s ease-in-out;
    }

    #hamburger.is-showing #hamburger__line-1 {
        transform: rotate(-45deg) translate(-5px, 6px);
    }

    #hamburger.is-showing #hamburger__line-2 {
        opacity: 0;
    }

    #hamburger.is-showing #hamburger__line-3 {
        transform: rotate(45deg) translate(-5px, -6px);
    }

