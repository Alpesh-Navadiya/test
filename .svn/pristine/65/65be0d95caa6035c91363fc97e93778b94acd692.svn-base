    }

    .navbar-mobile ul.is-showing {
        opacity: 1;
        right: 0;
        visibility: visible;
    }

    .navbar-mobile ul .avatar-icon {
        height: 16px;
    }
    .navbar-mobile a#dropdownwager {
        padding-left: 0;
