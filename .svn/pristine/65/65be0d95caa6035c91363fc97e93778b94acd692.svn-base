        opacity: 0.7;
        font-size: 0.75rem;
        margin-bottom: 1rem;
    }

    .nav__notifications .bell-icon {
        position: relative;
        margin-right: 6px;
    }

    .nav__notifications .bell-icon__circle {
        background-color: #F35800;
        position: absolute;
        top: -8px;
        right: -10px;
        border-radius: 10px;
        z-index: 4;
        font-size: 12px;
        padding: 4px;
        line-height: 8px;
    }

    .nav__notifications .headline {
        text-transform: uppercase;
        padding: 1rem 1rem;
        border-bottom: 1px solid rgba(255, 255, 255, 0.144);
        margin-bottom: 0;
    }

    .nav-notifications__content {
        padding: 1rem 1.5rem 1rem 1rem;
        max-height: 300px;
        overflow-y: scroll;
    }

    .nav__notifications .nav__notifications-list {
        width: 380px;
        display: none;
        background-color: rgb(20, 20, 20);
        position: absolute;
        top: 35px;
        right: 0;
        border-radius: 4px;
        box-shadow: -2px 6px 23px 0px rgba(0, 0, 0, 0.2);
    }

    .nav__notifications.is-showing .nav__notifications-list {
        display: block;
    }

    .nav__notification {
        padding-bottom: 1.5rem;
    }

    .nav__notification:last-child {
        padding-bottom: 0;
    }

    .notification__img {
        width: 50px;
        height: 50px;
        object-fit: cover;
        border-radius: 25px;
        margin-right: 10px;
    }

    .notification__text {
        margin-bottom: 1rem;
    }

    /* navbar account dropdown */
    .nav__account {
        position: relative;
        display: inline-block;
        cursor: pointer;
        padding-bottom: 0.5rem;
    }

    .nav__account ul {
        display: none;
        background-color: #3B3B40;
        position: absolute;
        top: 35px;
        right: 0;
