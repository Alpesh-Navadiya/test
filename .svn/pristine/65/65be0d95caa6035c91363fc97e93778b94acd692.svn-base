.button.button--main:hover {
    background-image: linear-gradient(180deg, #2e3857 0%, #212529 100%) !important;
    opacity: 1 !important;
}
.button.button--outline {
    border: 1px solid #20292f;
    background: none;
    color: #20292f;
}
.navbar .button {
    font-size: 15px;
    padding: 7px 18px;
    margin-top: -6px;
}
.button {
    border: 0;
    cursor: pointer;
    font-weight: bold;
    text-transform: uppercase;
    border-radius: 3px;
    transition: all 0.2s;
    font-family: "Rubik", sans-serif;
    font-size: 0.89rem;
}
.button.button--outline:hover {
    background: white;
    color: black;
    opacity: 1 !important;
}
.button.button--outline:hover {
    background: #20292f !important;
    border: 1px solid #4daaf2;
    color: white !important;
    opacity: 1 !important;
}